<Window x:Class="Praktikumsverwaltung_DesktopApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Praktikumsverwaltung_DesktopApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="1061.49" Width="1666.83">
    <Grid>
        <Menu x:Name="menuBar" DockPanel.Dock="Top" HorizontalAlignment="Left" Height="19" VerticalAlignment="Top" Width="583">
            <MenuItem Header="Entry" >
                <MenuItem x:Name="mItemEntryAdd" Header="Add" Click="mItemEntryAdd_Click" />
                <MenuItem x:Name="mItemEntryRemove" Header="Remove" />
                <MenuItem x:Name="mItemEntryEdit" Header="Edit" />
                <MenuItem x:Name="mItemEntryShow" Header="Show" />
            </MenuItem>
            <MenuItem Header="Profile" >
                <MenuItem x:Name="mItemProfileEdit" Header="Edit" />
                <MenuItem x:Name="mItemProfileShow" Header="Show" />
            </MenuItem>
        </Menu>
        <DataGrid x:Name="dataGrid" ItemsSource="{Binding}" HorizontalAlignment="Left" Margin="42,96,0,0" VerticalAlignment="Top" Height="265" Width="810"/>

    </Grid>
</Window>
